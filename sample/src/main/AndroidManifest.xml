<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.anychart.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".charts.PieChartActivity">
        </activity>
        <activity android:name=".charts.ColumnChartActivity">
        </activity>
        <activity android:name=".charts.LineChartActivity">
        </activity>
        <activity android:name=".charts.VennDiagramActivity">
        </activity>
    </application>

</manifest>